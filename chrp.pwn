		return SendError(playerid, "Tento hr·Ë nem· voln˝ slot na vozidlo!");
		
    new
	    model = INVALID_VEHICLE_ID;

	if(!IsNumeric(S_var))
	    model = ReturnVehicleID(S_var);
	else
	    model = strval(S_var);

