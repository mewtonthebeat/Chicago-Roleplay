			ReturnName(playerid)
		);

		SendSuccess(playerid, Query);
		SendSuccess(playerid, "Tvoja IP adresa je uû v datab·ze alebo je zo zahraniËia, budeö musieù vyËkaù na aktiv·ciu ˙Ëtu!");
		SendSuccess(playerid, "MÙûeö Ëakaù, alebo sa pripoj neskÙr!");
		SendError(playerid, " ");

	    new
	        String[ 144 ],
			admincount = 0;

		foreach(new i : Player)
		{
		    if(GetPlayerAdminLevel(i) > 0)
		        admincount ++;
		}

		format(String, sizeof String, "Pr·ve je online %d æudÌ, ktorÌ ti mÙûu skontrolovaù ûiadosù!", admincount);
		SendSuccess(playerid, String);
		
		playerWaiting[playerid] = true;
		
		FormatWarn(-1, "WhitelistWarn:", 0x9aec97ff, "Nov˝ hr·Ë si podal ûiadosù o aktivovanie ˙Ëtu! (%s, ID %d, /whitelist)", ReturnName(playerid), playerid);

