	format(S_str, 144, "* %s ensured the container. *", GetPlayerNameEx(playerid, USE_MASK));
	SendLocalMessage(playerid,ME_CHAT_RANGE,COLOR_VIOLET,S_str);

	new
		bool:isLast = true;

	for(new i; i < MAX_DUMPSTERS; i++)
	{
