			SCFM(playerid, COLOR_GREEN, "[ ! ] {ffffff}Toto vozidlo má %d litrov paliva, natankovaný je %s!", vEnum[vehicleid][v_Fuel], palivo);

	   		switch(vehicleFuelTypes[GetVehicleModel(vehicleid)-400])
