    new
		id = -1,
		typ = -1;

	for (new i; i < MAX_FUEL_STATIONS; i++)
	{
		if(strcmp(fuelEnum[i][fuel_cislo_boxu], S_var1, true))
		    continue;

		id = i;
		break;
	}

	if(id == -1)
	    return SendError(playerid, "Box s t˝mto oznaËenÌm neexistuje!");

	if(!strcmp(S_var2, "benzin", true))
	    typ = 0;
	else if(!strcmp(S_var2, "diesel", true))
	    typ = 1;
    else if(!strcmp(S_var2, "kerosin", true))
	    typ = 2;
    else if(!strcmp(S_var2, "lpg", true))
	    typ = 3;
	else
	    return SendClientSyntax(playerid, "/doplnitpumpu [typ (benzin/diesel/kerosin/lpg)] [oznaËenie boxu]");
	    
