	    return SCFM(playerid, COLOR_ADMINCMD, "[ ! ] Nemôe nabi to¾ko nábojov, maximálne môe nabi %d nábojov!", MaxAmmo - GetPlayerAmmo(playerid));
	    
	SetPlayerAmmo(playerid, GetPlayerWeapon(playerid), GetPlayerAmmo(playerid) + I_var);
	
	SetPlayerInventoryItem(playerid, inv_naboje, GetPlayerInventoryItem(playerid, inv_naboje) - I_var);
	
	SCFM(playerid, /*COLOR_COMMANDUSE*/0xD0D0D0FF, "[ ! ] Zbraò %s nabitá s %d nábojmi! Nové náboje: %d!", GetWeaponNameEx(GetPlayerWeapon(playerid)), I_var, GetPlayerAmmo(playerid) + I_var);
	
