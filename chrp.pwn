					format(palivo, sizeof palivo, "nemá");

	            case 1:
					format(palivo, sizeof palivo, "benzín");

	            case 2:
					format(palivo, sizeof palivo, "diesel");

	            case 3:
					format(palivo, sizeof palivo, "kerosin");

	            case 4:
