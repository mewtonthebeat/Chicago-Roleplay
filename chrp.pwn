	{
		PlayerTextDrawDestroy(playerid, tuttd[playerid][0]);
		PlayerTextDrawDestroy(playerid, tuttd[playerid][1]);
		PlayerTextDrawDestroy(playerid, tuttd[playerid][2]);
		
		isTutor[playerid] = false;
		
	    TogglePlayerSpectating(playerid, false);
	    return 1;
	}
	
	if(id != 0)
	{
		PlayerTextDrawHide(playerid, tuttd[playerid][0]);
		PlayerTextDrawHide(playerid, tuttd[playerid][1]);
