	if(!IsPlayerLogged(I_var))
	    return SendError(playerid, "Tento hr·Ë nie je v hre!");
	    
    if(GetPlayerDonatorLevel(I_var) < 1 && CountPlayerVehicles(I_var) >= MAX_PLAYER_VEHICLES)
    	return SendError(playerid, "Tento hr·Ë nem· voln˝ slot na vozidlo!");

