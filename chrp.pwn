stock ac_GetWeaponSlot(weaponid)
{
	new
		slot;

	switch(weaponid)
	{
		case 0,1: slot = 0;
		case 2 .. 9: slot = 1;
		case 10 .. 15: slot = 10;
		case 16 .. 18, 39: slot = 8;
		case 22 .. 24: slot =2;
		case 25 .. 27: slot = 3;
		case 28, 29, 32: slot = 4;
		case 30, 31: slot = 5;
		case 33, 34: slot = 6;
		case 35 .. 38: slot = 7;
		case 40: slot = 12;
		case 41 .. 43: slot = 9;
		case 44 .. 46: slot = 11;
	}
	return slot;
}

stock ac_GivePlayerWeapon(playerid, weaponid, ammo)
{
	ac_immunity[playerid] = true;

	ac_playerGuns[playerid][weaponid] = 1;
	
	new
		ac_oldGun,
		ac_oldAmmo;

	GetPlayerWeaponData(playerid, ac_GetWeaponSlot(weaponid), ac_oldGun, ac_oldAmmo);
	GivePlayerWeapon(playerid, weaponid, ammo);
	
	if(ac_oldGun != 0 && ac_oldGun != weaponid)
	{
		ac_playerGuns[playerid][ac_oldGun] = 0;
	}
	
	SetTimerEx("ac_timer_DisableImmunity", 2000, false, "i", playerid);
	return 1;
}
#if defined _ALS_GivePlayerWeapon
    #undef GivePlayerWeapon
#else
    #define _ALS_GivePlayerWeapon
#endif
#define GivePlayerWeapon ac_GivePlayerWeapon

stock ac_ResetPlayerWeapons(playerid)
{

    ResetPlayerWeapons(playerid);

	for(new i; i < 50; i++)
	{
		ac_playerGuns[playerid][i] = 0;
	}
	return 1;
}
#if defined _ALS_ResetPlayerWeapons
    #undef ResetPlayerWeapons
#else
    #define _ALS_ResetPlayerWeapons
#endif
#define ResetPlayerWeapons ac_ResetPlayerWeapons

stock ac_RemovePlayerWeapon(playerid, weaponid)
{
	ac_immunity[playerid] = true;

	new
		ac_weapons[12],
		ac_ammo[12];

  	for(new slot = 0; slot != 12; slot++)
	{
    	new
			wep,
			ammo;

    	GetPlayerWeaponData(playerid, slot, wep, ammo);

    	if(wep != weaponid)
		{
      		GetPlayerWeaponData(playerid, slot, ac_weapons[slot], ac_ammo[slot]);
    	}
  	}

	ResetPlayerWeapons(playerid);

	for(new slot = 0; slot != 12; slot++)
	{
    	GivePlayerWeapon(playerid, ac_weapons[slot], ac_ammo[slot]);
    	ac_playerGuns[playerid][ac_weapons[slot]] = 1;
  	}

	ac_playerGuns[playerid][weaponid] = 0;

	SetTimerEx("ac_timer_DisableImmunity", 1000, false, "i", playerid);
	return 1;
}
#if defined _ALS_RemovePlayerWeapon
    #undef RemovePlayerWeapon
#else
    #define _ALS_RemovePlayerWeapon
#endif
#define RemovePlayerWeapon ac_RemovePlayerWeapon

stock ac_IsPlayerWeaponCheating(playerid, &r_weaponid = 0, &r_ammo = 0)
{
	if(!IsPlayerConnected(playerid))
		return 0;

	if(ac_immunity[playerid] == true)
	    return 0;

	for(new i; i < 12; i++)
	{
		new
			wep,
			ammo;

		GetPlayerWeaponData(playerid, i, wep, ammo);

		if(wep > 0 && ammo > 0)
		{
			if(ac_playerGuns[playerid][wep] != 1)
			{
				r_weaponid = wep;
				r_ammo = ammo;
				return 1;
			}
		}
	}

	return 0;
}
