	}
	else if(!strcmp(S_var, "olej", true))
	{
		if(vehicleid == -1)
		    return SendError(playerid, "Nie je tu iadne vozidlo!");

		if(GetPlayerInventoryItem(playerid, inv_carOil) < 1)
		    return SendError(playerid, "Nemá v inventári f¾au motorového oleja!");

