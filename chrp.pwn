/*

	/setprava COMMAND

	< ENGLISH >
	    Can edit admin level.


	< SLOVAK >
	    Upravuje admin level hráèa.

*/

YCMD:setprava(playerid, params[], help)
{

	if(GetPlayerAdminLevel(playerid) < 6)
	    return SendClientCantUseCommand(playerid);

	new
		I_var,
		S_var1[64],
		lv;

	if(sscanf(params, "ds[64]D(-1)", I_var, S_var1,lv))
		return SendClientSyntax(playerid, "/setprava [id] [hrac - helper - moderator - newbie_admin - junior_admin - admin - senior_admin - ladmin]");

	new
 		I_level = 11;

	if(!strcmp(S_var1, "helper", true)) I_level = 1; //hel
	else if(!strcmp(S_var1, "moderator", true)) I_level = 2; //mod
	else if(!strcmp(S_var1, "newbie_dmin", true)) I_level = 4; //alvl 1
	else if(!strcmp(S_var1, "junior_admin", true)) I_level = 5; //alvl 2
	else if(!strcmp(S_var1, "advanced_admin", true)) I_level = 6; //alvl 3
	else if(!strcmp(S_var1, "admin", true)) I_level = 7; //alvl 4
	else if(!strcmp(S_var1, "senior_admin", true)) I_level = 8; //alvl 5
	else if(!strcmp(S_var1, "ladmin", true)) I_level = 9; //alvl 6
	else if(!strcmp(S_var1, "custom", true)) I_level = 11;
	else if(!strcmp(S_var1, "hrac", true)) I_level = 0;
	else return SendClientSyntax(playerid, "/setprava [id] [hrac - helper - moderator - newbie_admin - junior_admin - admin - senior_admin - ladmin]");

	if(I_level == 11 && lv < 1)
	    return SendClientSyntax(playerid, "/setprava [id] [hrac - helper - moderator - newbie_admin - junior_admin - admin - senior_admin - ladmin]");

	if(!IsPlayerLogged(I_var))
	    return SendClientPlayerOffline(playerid);

	if(I_level != 11)
		SetPlayerAdminLevel(I_var, I_level);
	else
	    SetPlayerAdminLevel(I_var, I_level);


	new
	    S_title[64],
		S_string[144];

    switch(I_level) {
        case 0:
            strcat(S_title, "Hráè");
    	case 1:
			strcat(S_title, "Helper");
   		case 2:
			strcat(S_title, "Moderátor");
        case 4,5,6,7,8:
			strcat(S_title, "Administrator");
        case 9:
			strcat(S_title, "Lead Administrator");

        default:
        {
			format(S_title, 64, "Netandartný post");
		}

	}

	format(S_string, 144, "> Nastavil si oprávnenie %s hráèovi %s!", S_title, GetPlayerNameEx(I_var, NO_MASK));
