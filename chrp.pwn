function timer_Speedometer()
{
	foreach( new i : Player )
	{
		if(!IsPlayerInAnyVehicle(i) || (GetPlayerVehicleSeat(i) != 0 && GetPlayerVehicleSeat(i) != 1) || opt_tachometer[i] == 2 || GetVehicleModel(GetPlayerVehicleID(i)) == 0)
		{

		    for(new x; x < 9; x ++)
				PlayerTextDrawHide(i, PTD_speedometer[i][x]);
				
            for(new ix; ix < 7; ix++)
				TextDrawHideForPlayer(i, tachometer[ix]);
				
            for(new xi; xi < SPEEDO_HAND_CIRCLES; xi++)
                PlayerTextDrawHide(i, tachometer_hand[i][xi]);
                
            PlayerTextDrawHide(i, tachometer_mph[i]);

			continue;
		}

        if(!IsPlayerInDynamicArea(i, PCMechanicArea, 0) && (upgradeShop_carColor[GetPlayerVehicleID(i)][0] != -1 || upgradeShop_carColor[GetPlayerVehicleID(i)][1] != -1))
		{
		    ChangeVehicleColor(GetPlayerVehicleID(i), vEnum[GetPlayerVehicleID(i)][v_Color_1], vEnum[GetPlayerVehicleID(i)][v_Color_2]);

	        upgradeShop_carColor[GetPlayerVehicleID(i)][0] = -1;
	        upgradeShop_carColor[GetPlayerVehicleID(i)][1] = -1;

			DisableRemoteVehicleCollisions(i, 0);
		}

		if(GetPlayerVehicleSeat(i) == 0)
		{
		    switch(GetVehicleModel(GetPlayerVehicleID(i)))
		    {
		        case
		            481,
		            509,
		            510:
				{
				    SetVehicleParams(GetPlayerVehicleID(i), VEHICLE_TYPE_ENGINE, 1);
				    SetVehicleParams(GetPlayerVehicleID(i), VEHICLE_TYPE_DOORS, 0);
				}
		    }
		}

		new
		    Float:v_HP1,
		    Float:v_HP2;

		vhac_GetVehicleHealth(GetPlayerVehicleID(i), v_HP1);
		GetVehicleHealth(GetPlayerVehicleID(i), v_HP2);

		if(v_HP2 < v_HP1 && v_HP2 > 299 && veh_IsDeath[GetPlayerVehicleID(i)] == false && vhac_immunity[GetPlayerVehicleID(i)] == false)
		{
			SetVehicleHealth(GetPlayerVehicleID(i), v_HP2);
		}
		else if(v_HP2 < 298 && veh_IsDeath[GetPlayerVehicleID(i)] == false)
		{
		    SetVehicleHealth(GetPlayerVehicleID(i), 299.1);
		}


		if(v_HP2 < 300.0 && GetVehicleParams(GetPlayerVehicleID(i), VEHICLE_TYPE_ENGINE) == 1)
		{
		
		    if(GetVehicleModel(GetPlayerVehicleID(i)) == 481 || GetVehicleModel(GetPlayerVehicleID(i)) == 509 || GetVehicleModel(GetPlayerVehicleID(i)) == 510)
		    {
				vhac_SetVehicleHealth(GetPlayerVehicleID(i), 1000.0);
		    }
		    else
		    {
			    SetVehicleParams(GetPlayerVehicleID(i), VEHICLE_TYPE_ENGINE, 0);

