	if(!IsPlayerLogged(I_var))
	    return SendError(playerid, "Tento hráè nie je v hre!");

	if(!IsPlayerNearPlayer(I_var, playerid, 3.5))
	    return SendError(playerid, "Tento hráè je moc ïaleko!");

