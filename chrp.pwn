 		if(strlen(barrierEnum[i][barri_PlacedBy]) < 2)
   			break;

        if(strlen(barrierEnum[i][barri_PlacedBy]) >= MAX_BARRIERS)
            return SendError(playerid, "Nie je voln˝ slot na bariÈru!");

		continue;
	}
	
	for(new i; i < MAX_BARRIERS; i++)
 	{
  		if(strlen(barrierEnum[i][barri_PlacedBy]) > 1)
    		continue;
    		
		new Float:X, Float:Y, Float:Z, Float:A;

		GetPlayerPos(playerid, X, Y, Z);
		GetPlayerFacingAngle(playerid, A);

		X += 1.5 * floatsin(-A, degrees);
		Y += 1.5 * floatcos(-A, degrees);


		format(barrierEnum[i][barri_PlacedBy], 25, ReturnName(playerid));

		barrierEnum[i][barri_LabelUsed] = 5;
		barrierEnum[i][barri_PosX] = X;
		barrierEnum[i][barri_PosY] = Y;
		barrierEnum[i][barri_PosZ] = Z;

		barrierEnum[i][barri_PosVW] = GetPlayerVirtualWorld(playerid);
		barrierEnum[i][barri_PosINT] = GetPlayerInterior(playerid);

        IsEditingBarrier[playerid] = true;
		editingpaskaid{playerid} = barrierEnum[i][barri_BarrierID] = CreateLineSegment(
			19834, 2.335,
			X, Y, Z,
			X, Y, Z,
			0.0, 0.0, 90.0,
			.objlengthoffset = -(2.335/2)
		);
		
		SetLineSegmentMaxLength(editingpaskaid{playerid}, 40.0);

  		Streamer_UpdateEx(playerid, X,Y+0.1,Z);

		EnterInfo(playerid, "~w~Zacal si tahat pasku, chod na poziciu kde ju chces dotiahnut a napis ~b~/ppaska~w~!", 30000, 0);

		return 1;
	}

	SendError(playerid, "Nie je voæn˝ slot na p·sku!");
	return 1;
}

YCMD:zosrotovatauto(playerid, params[], help)
{
    if(GetPlayerStaffLevel(playerid) < 4) return SendClientCantUseCommand(playerid);

    new
		I_var[30]
	;

	if(sscanf(params,"s[30]",I_var))
		return SendClientSyntax(playerid, "/zosrotovatauto [spz/toto]");

	new vId = -1;

	if(strcmp(I_var, "toto", false))
	{

		new bool:vFound = false;

		foreach( new vehicleid : Vehicle )
		{
		    if(!IsValidVehicle(vehicleid))
		        continue;

			if(!strlen(vEnum[vehicleid][v_SPZ]))
			    continue;

			if(strcmp(vEnum[vehicleid][v_SPZ], I_var, false))
			    continue;

			vFound = true;
			vId = vehicleid;
		}

		if(vFound == false)
		    return SendError(playerid, "Vozidlo s touto znaËkou neexistuje!");
	}
	else
	{
	    if(!IsPlayerInAnyVehicle(playerid))
	        return SendError(playerid, "Nie si v ûiadnom vozidle!");

		vId = GetPlayerVehicleID(playerid);
	}

	if(strlen(vEnum[vId][v_Owner]) < 2)
	    return SendError(playerid, "Toto vozidlo nemÙûeö zoörotovaù!");

	new
	    vehprice = 0,
	    model = GetVehicleModel(vId)
	;

	if(wats_vehiclePrices[model-400][2] != 1 && wats_vehiclePrices[model-400][0] >= vehprice)
	    vehprice = wats_vehiclePrices[model-400][0];

	if(bigmikes_vehiclePrices[model-400][2] != 1 && bigmikes_vehiclePrices[model-400][0] >= vehprice)
		vehprice = bigmikes_vehiclePrices[model-400][0];
	if(mrgrants_vehiclePrices[model-400][2] != 1 && mrgrants_vehiclePrices[model-400][0] >= vehprice)
	    vehprice = mrgrants_vehiclePrices[model-400][0];

    ex_GivePlayerMoney(playerid, floatround(float(vehprice) / 50.0, floatround_round));

    SCFM(playerid, 0xD0D0D0FF, "[ ! ] Zoörotoval si vozidlo %s (%s), patrilo hr·Ëovi %s a dostal by zaÚ %d$!", GetVehicleName(vId), vEnum[vId][v_SPZ], vEnum[vId][v_Owner], floatround(float(vehprice) / 50.0, floatround_round));
    FormatWarn(playerid, "AdmCmd:", COLOR_ADMINCMD, "%s zoörotoval vozidlo %s (%s), patrilo hr·Ëovi %s a dostal by zaÚ %d$!", GetPlayerNameEx(playerid, USE_PUREIDNAME), GetVehicleName(vId), vEnum[vId][v_SPZ], vEnum[vId][v_Owner], floatround(float(vehprice) / 50.0, floatround_round));
	FormatLog(log_type_buythings, "%s(%s - %s) admin-zosrotoval vozidlo so SPZ %s, dostal by %d$.", ReturnName(playerid), ReturnMaster(playerid), ReturnIP(playerid), vEnum[vId][v_SPZ], floatround(float(vehprice) / 50.0, floatround_round));

    new mysql[144];

    mysql_format(MYSQL, mysql, sizeof mysql, "DELETE FROM char_vehicles WHERE SPZ = '%e'", vEnum[vId][v_SPZ]);
    mysql_query(MYSQL, mysql, false);
    DestroyVehicle(vId);

    return 1;
}

YCMD:prepisauto(playerid, params[], help)
