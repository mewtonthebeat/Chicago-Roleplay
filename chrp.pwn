IsAbleToApplyOnFootAnim(playerid)
{
	if(IsPlayerInAnyVehicle(playerid)) return 0;
	
	switch(g_I_playerCheckpoint[playerid])
	{
		case checkp_brigada_krabice_1, checkp_brigada_krabice_2,
				checkp_brigada_krabice_mg_1, checkp_brigada_krabice_mg_2,
				checkp_brigada_krabice_od1_1, checkp_brigada_krabice_od1_2,
				checkp_brigada_krabice_od2_1, checkp_brigada_krabice_od2_2:

		{
			return 0;
		}
	}
	
	if(GetPlayerFactionType(playerid) == FACTION_TYPE_PIZZADELIVERY && isDoingSWJob[playerid] == true && IsPlayerAttachedObjectSlotUsed(playerid, 9))
	    return 0;
	
	else return 1;
}

