    lastTimeChange = hour;

	SetWorldTime(hour);
	gActualTime = hour;

	return 1;

}

new
	lastPayDay = -1,
	lottery = 0;
	
func_timer_CheckPayDay()
{

    new
	    hour,
	    minu;

	gettime(hour, minu);

    if(lastPayDay != hour && minu == 0)
	{
	
	    new
			lotteryCount,
			lotteryNumber = rand(1,100),
			lotteryWinner;
	
	    lastPayDay = hour;
	    foreach(new i : Player)
		{
		
		    if(GetPlayerInventoryItem(i, inv_lotteryTicket) == lotteryNumber)
		    {
				lotteryCount ++;
				lotteryWinner = i;
			}
		
	    	if(g_I_paydayTime[i] < 1740)
	    	{
	    	    SendClientMessage(i, /*COLOR_COMMANDUSE*/0xD0D0D0FF, "[ ! ] You didn't play enought time, to receive your payday.");
	    	    g_B_canHavePayDay[i] = true;
	    	    g_B_canPickUpPayDay[i] = false;
	    	    g_I_VyplataExpire[i] = 0;
	    	    
	    	    new
				    finalPayday,
				    finalDane,
				    finalDonor,


				    finalCash;

				finalPayday = g_I_Vyplata[i];

				switch( GetPlayerDonatorLevel(i) )
				{
				    case 0:
						finalDonor = 0;

					case 1:
						finalDonor = 750;

					case 2:
						finalDonor = 1500;

					case 3:
						finalDonor = 3000;

					default:
						finalDonor = 0;
				}

				foreach( new x : Houses )
				{

					if(strcmp(houseEnum[x][ho_Owner], ReturnName(i), false))
					    continue;

					finalDane += floatround(float(houseEnum[x][ho_BuyPrice]) / 10000.0, floatround_round);
				}

				foreach( new x : Businesses )
				{
					if(strcmp(businessEnum[x][bz_Owner], ReturnName(i), false))
					    continue;

					finalDane += floatround(float(businessEnum[x][bz_BuyPrice]) / 10000.0, floatround_round);

					finalPayday += rand(750, 1550);
				}

				if(GetPlayerFactionType(i) != 0 && GetPlayerFaction(i) < sizeof(factionPayday))
				{
				    finalPayday += floatround( factionPayday[GetPlayerFaction(i)] * (GetPlayerFactionRank(i) * 1.5), floatround_round);
				}
				else
				{
				    finalPayday += rand(250, 400);
				}

				finalCash += (finalPayday + finalDonor - finalDane);
				
				if(finalCash < 0)
				{
				    GivePlayerFine(ReturnName(i), finalCash, "Arrear", "Government");
				    SCFM(i, COLOR_WHITE, "| You can pay your arrear %d$ in City Hall. |", finalCash);
				}
	    	}
	    	else
	    	{

				if(Vyplata_TYP[i] == 0)
				{
				    SCFM(i, /*COLOR_COMMANDUSE*/0xD0D0D0FF, "[ ! ] You received your payday! You can pickup your payday in City Hall by command /signcheck [number], if you want!");

				
					g_B_canHavePayDay[i] = true;
		    	    g_B_canPickUpPayDay[i] = true;

		    	    g_I_VyplataExpire[i] = gettime() + 1800;

		    	    payDayCheck[i] = rand(1, 9999);

					new
						tdStr[64];

					format(tdStr, sizeof tdStr, "Signcheck: ~w~%04d", payDayCheck[i]);

		    	    PlayerTextDrawShow(i, TD_vyplaty[i][0]);
		    	    PlayerTextDrawSetString(i, TD_vyplaty[i][0], tdStr);
		    	    PlayerTextDrawShow(i, TD_vyplaty[i][0]);
				}
				else
				{
				    GivePlayerPayDay(i, 1);
				}
	    	}
		}
		
		if(lottery == 0)
		    lottery = rand(3000,32500);
		
		if(lotteryCount == 0)
		{
		    foreach( new i : Player )
		    {
		        SetPlayerInventoryItem(i, inv_lotteryTicket, 0);
		        
          		SCFM(i, COLOR_LOTTERY, "[ LOTTERY ] There was $%d USD in jackpot, nobody guess number %d!", lottery, lotteryNumber);
		    }
		}
		else if(lotteryCount == 1)
		{
		    foreach( new i : Player )
		    {
		        SetPlayerInventoryItem(i, inv_lotteryTicket, 0);

				SCFM(i, COLOR_LOTTERY, "[ LOTTERY ] There was $%d USD in jackpot, and correct number was %d! Congratulations for the winner.", lottery, lotteryNumber);
				if(i == lotteryWinner)
				{
				    SCFM(i, COLOR_LOTTERY, "[ LOTTERY ] Congratulations, you win $%d USD! Enjoy!", lottery);
				    ex_GivePlayerMoney(i, lottery);
				}
			}
		}
		else
		{
		    foreach( new i : Player )
		    {
		    
		        if(GetPlayerInventoryItem(i, inv_lotteryTicket) == lotteryNumber)
		        {
		            SCFM(i, COLOR_LOTTERY, "[ LOTTERY ] There was $%d USD in jackpot, and correct number was %d! Congratulations for the winner.", lottery, lotteryNumber, lotteryCount);
                    SCFM(i, COLOR_LOTTERY, "[ LOTTERY ] Congratulations, you win $%d USD! Enjoy!", floatround(float(lottery) / float(lotteryCount), floatround_round));
                    ex_GivePlayerMoney(i, floatround(float(lottery) / float(lotteryCount), floatround_round));
				}
				else
				    SCFM(i, COLOR_LOTTERY, "[ LOTTERY ] There was $%d USD in jackpot, and correct number was %d! Congratulations for the winner.", lottery, lotteryNumber, lotteryCount);
		    
		        SetPlayerInventoryItem(i, inv_lotteryTicket, 0);
			}
		}
		
		lottery = 0;
	}

	if(lastPayDay == hour && minu == 30)
	{

	    lastPayDay = -1;
	    lottery = rand(3000,32500);

	    foreach(new i : Player)
	    {
	        if(g_B_canPickUpPayDay[i] == true)
	        {
	            PlayerTextDrawHide(i, TD_vyplaty[i][0]);
	            payDayCheck[i] = 0;
	            g_I_VyplataExpire[i] = 0;
	            g_B_canPickUpPayDay[i] = false;
	            SCFM(i, /*COLOR_COMMANDUSE*/0xD0D0D0FF, "[ ! ] Time for payday has now expired, %d$ will be added to your next payday!", floatround(g_I_Vyplata[i] / 100 * 40, floatround_round));
	        }
	        
	        SCFM(i, COLOR_LOTTERY, "[ LOTTERY ] Jackpot $%d USD, buy ticket in General Store and join, draw will be in 30min!", lottery);
	    }
	}

	return;

}

