	}

	if(strlen(street) < 7)
	    fontsize = 30;

	streetsignEnum[i][znacka_ObjectID2] = CreateDynamicObject(19477, exX, exY, Z + 2.677186, RX, RY, RZ - 90.000000, -1, -1, -1, 40.00, 40.00);
