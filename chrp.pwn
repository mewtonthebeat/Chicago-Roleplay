		return SendError(playerid, "Tak˝to model neexistuje!");
	    
    new
		vehicleid,
		SPZ[24];

	vehicleid = CreateVehicle(model, 641.148, -499.903, 16.078, 270.0, 1, 1, -1, 0);

	strcat(SPZ, SPZ_PATTERN);

	for(new i; i < sizeof(SPZ); i++)
