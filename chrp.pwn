	/////////////////////////////////////////////////////////////////////////////////////////////////////////////////
	/////////////////////////////////////////////////////////////////////////////////////////////////////////////////
	streetsignEnum[i][znacka_ObjectID1] = CreateDynamicObject(19981, X, Y, Z, RX, RY, RZ, -1, -1, -1, 200.00, 200.00);

	return 1;

