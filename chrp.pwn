			    continue;

			SetVehicleToRespawn(i);

			RepairVehicle(i);
			vEnum[i][v_Fuel] = 100;
			vEnum[i][v_Battery] = 1000;
			vEnum[i][v_Oil] = 100.0;
			vEnum[i][v_IsBeingRespawned] = true;

		}

		foreach( new x : Player )
		{

