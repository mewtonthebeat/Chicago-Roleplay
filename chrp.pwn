		{
			spec_Vehicle[playerid][0] = GetPlayerVehicleID(playerid);
			spec_Vehicle[playerid][1] = GetPlayerVehicleSeat(playerid);
		}
		else
		    spec_Vehicle[playerid][0] = -1;

		ahac_GetPlayerHealth(playerid, spec_Vitals[playerid][0]);
		ex_GetPlayerHunger(playerid, spec_Vitals[playerid][1]);
		ahac_GetPlayerArmour(playerid, spec_Vitals[playerid][2]);

		for(new i; i < 12; i++)
	    {

	        GetPlayerWeaponData(playerid, i, lg_tempGun[playerid][i][0], lg_tempGun[playerid][i][1]);

		}
	}

	isSpectating[playerid] = true;

	spec_Player[playerid] = I_var;

	TogglePlayerSpectating(playerid, true);

	SetPlayerVirtualWorld(playerid, GetPlayerVirtualWorld(I_var));
	SetPlayerInterior(playerid, GetPlayerInterior(I_var));

	PlayerSpectatePlayer(playerid, I_var);

	FormatWarnEx(playerid,2, "AdmCmd:", COLOR_ADMINCMD, "%s zaËal spectatovaù hr·Ëa %s.", GetPlayerNameEx(playerid, USE_PUREIDNAME), GetPlayerNameEx(I_var, USE_PUREIDNAME));

	PlayerTextDrawShow(playerid, td_spectateTD0[playerid]);
	PlayerTextDrawShow(playerid, td_spectateTD1[playerid]);
	PlayerTextDrawShow(playerid, td_spectateTD2[playerid]);
	PlayerTextDrawShow(playerid, td_spectateTD3[playerid]);
	PlayerTextDrawShow(playerid, td_spectateTD4[playerid]);
	PlayerTextDrawShow(playerid, td_spectateTD5[playerid]);
	PlayerTextDrawShow(playerid, td_spectateTD6[playerid]);

	return 1;
}

