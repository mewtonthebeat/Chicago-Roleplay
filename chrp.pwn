	);
	
	for( new i; i < MAX_FACTIONS; i++)
	{
	    if(strlen(factionString[i]) < 3)
	        continue;

		strcat(finalString, factionString[i]);
	}

	ShowPlayerDialog(
	    playerid,
	    did_Admins,
	    DIALOG_STYLE_MSGBOX,
	    "FRAKCIE",
	    finalString,
	    "OK",
	    ""
	);

	return 1;
